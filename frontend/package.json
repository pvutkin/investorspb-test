{
  "name": "startup-platform-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "docker:build": "docker build -t startup-platform-frontend .",
    "docker:run": "docker run -p 3000:80 startup-platform-frontend",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "deploy:prepare": "npm run build && npm run test",
    "deploy:docker": "npm run docker:build && npm run docker:run"
  },
  "dependencies": {
    "@eslint/config-array": "^0.21.0",
    "@eslint/object-schema": "^2.1.6",
    "@tanstack/react-query": "^5.85.6",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.8.2",
    "react-select": "^5.10.2",
    "react-toastify": "^11.0.5",
    "rimraf": "^6.0.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "vite": "^7.1.4"
  }
}
