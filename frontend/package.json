{
  "name": "startup-platform-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "docker:build": "docker build -t startup-platform-frontend .",
    "docker:run": "docker run -p 3000:80 startup-platform-frontend",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "deploy:prepare": "npm run build && npm run test",
    "deploy:docker": "npm run docker:build && npm run docker:run"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "axios": "^1.3.0",
    "react-query": "^3.39.0",
    "react-hook-form": "^7.43.0",
    "react-modal": "^3.16.1",
    "socket.io-client": "^4.6.0",
    "date-fns": "^2.29.0",
    "clsx": "^1.2.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-select": "^5.7.0",
    "react-toastify": "^9.1.0",
    "framer-motion": "^10.0.0",
    "lucide-react": "^0.263.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "vite": "^4.2.0"
  }
}